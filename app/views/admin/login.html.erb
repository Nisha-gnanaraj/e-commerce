<div class="container">
<div class="row rowup">
<div class="col-sm">
</div>
<div class="col-sm">
<h4 class="text-center down_space">Admin Login</h4>
<%= form_with do |f| %>
    <%= flash[:notice] %>
<div class="form-group">
<%= f.label 'Name' %>
<%= f.text_field :name , :class => 'form-control'%>
</div>
<div class="form-group">
<%= f.label :password %>
<%= f.password_field :password, :class => 'form-control' %><br>
</div>
<div class="form-group">
<%= f.submit :Submit , :class => 'btn btn-primary'%>
</div>
<% end %>
</div>
<div class="col-sm">
</div>
</div>
</div>